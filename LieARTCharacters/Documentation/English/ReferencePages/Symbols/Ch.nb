 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox["\[Sum]", 
     RowBox[{"w", "\[Element]", "W"}]], 
    RowBox[{
     UnderoverscriptBox["\[Product]", 
      RowBox[{" ", 
       RowBox[{"i", "=", "1"}]}], 
      RowBox[{" ", "r"}]], 
     SubsuperscriptBox["x", "i", 
      SubscriptBox["w", "i"]]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"ff15e1e2-7a02-4e3f-be9d-70854ad64c70"],
